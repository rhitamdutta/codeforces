#include <iostream>

using namespace std;

int main()
{
    int a;
    cin>>a;
    int ar[3];
    int c;
    int n=0;
    for(int i=1;i<=a;i++)
    {
        c=0;
        for(int j=0;j<3;j++)
        {
            cin>>ar[j];
        }
        for(int k=0;k<3;k++)
        {
            if(ar[k]==1){
            c++;}
            else
                continue;
        }
        if(c==2 || c==3)
        {
            n++;
        }
    }
    cout<<n;
}
